<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neko.seed.traffic.mapper.ServiceTodoMapper">

    <select id="getLeastOne"  resultType="com.neko.seed.traffic.entity.ServiceTodo">
        select *, max(rec_time) from traffic_service_todo where rec_road_section_name= #{roadName} limit 1;
    </select>
</mapper>
